{
    "extends": [
        "react-app",
        "react-app/jest"
      ],
      "rules": {
        "no-unused-vars": "warn",
        "prefer-const": "warn",
        "jsx-a11y/alt-text": "off",
        "no-console": "warn",
        "quotes": ["warn", "double"],
        "semi": ["warn", "always"]
      }
}